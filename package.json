{
  "name": "weatherapp",
  "version": "1.0.0",
  "description": "Weather application that shows also where are you in the map",
  "default": "./src/**/index.html",
  "scripts": {
    "watch": "tailwindcss -i ./src/**/style.css -o ./dist/output.css --watch",
    "tailwind": "tailwindcss -i ./src/**/style.css -o ./dist/output.css",
    "start": "parcel ./src/**/index.html",
    "build": "parcel build ./src/**/index.html --dist-dir ./dist"
  },
  "author": "Marco Di Fatta",
  "license": "ISC",
  "devDependencies": {
    "parcel": "^2.9.3",
    "process": "^0.11.10",
    "tailwindcss": "^3.3.3"
  },
  "dependencies": {
    "@ionic/cli": "^7.1.1",
    "core-js": "^3.32.0",
    "leaflet": "^1.9.4",
    "regenerator-runtime": "^0.14.0"
  }
}
